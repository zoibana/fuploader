{
  "name": "@zoibana/fuploader",
  "version": "0.0.10",
  "description": "Best amazing HTML5 async file uploader plugin",
  "main": "dist/js/Fuploader.js",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "keywords": [
    "file",
    "upload",
    "image",
    "drag-n-drop",
    "image upload",
    "file upload",
    "upload file",
    "files"
  ],
  "devDependencies": {
    "laravel-mix": "^6.0.42",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0"
  },
  "author": "zoibana@gmail.com",
  "license": "ISC",
  "repository": "git@github.com:zoibana/fuploader.git",
  "dependencies": {
    "sortablejs": "^1.14.0"
  }
}
